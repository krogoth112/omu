<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="img">
	<typeAlias alias="ImgVo" type="com.bit2015.omu.vo.ImgVo"/>
	<insert id="insert">
		<![CDATA[
			insert into img values(img_no_seq.nextval, #imageURL#)
		]]>
	</insert>
	
	
	<insert id="insert" parameterClass="ImgVo">
		<![CDATA[
			insert into Img values(img_no_seq.nextval, #imageUrl#)
		]]>
	</insert>
	
	
	
</sqlMap>
